<options>

<!-- ctp_tools -e coupling options.xml -->
<coupling help="Electronic couplings from log and orbital files (GAUSSAIN, TURBOMOLE, NWChem)" label="calc:overlap">
	
	<package help="XML file with the options for the first-principles package">gaussian_coupling.xml</package>

	<degeneracy help="Criterium for the degeneracy of two levels" unit="eV" default="0">0</degeneracy>

	<levels help="Output HOMO, ..., HOMO-levels; LUMO, ..., LUMO+levels" default="3">3</levels>

	<moleculeA help="">
		<log help="Log file of molecule A" default="A.log">A.log</log>
		<orbitals help="Orbitals file" default="A.orb">A.orb</orbitals>
		<levels help="Output HOMO, ..., HOMO-levels; LUMO, ..., LUMO+levels" default="3">3</levels>
		<trim help="" default="2" >2</trim>
	</moleculeA>

	<moleculeB>
		<log help="Log file of molecule B" default="B.log">3</log>
		<orbitals help="Orbitals file" default="B.orb">3</orbitals>
		<levels help="Output HOMO, ..., HOMO-levels; LUMO, ..., LUMO+levels" default="3">3</levels>
		<trim help="" default="2" >2</trim>
	</moleculeB>

	<dimerAB>
		<log help="Log file of dimer AB" default="AB.log">3</log>
		<orbitals help="Orbitals file" default="A.orb">3</orbitals>
	</dimerAB>

</coupling>

</options>


